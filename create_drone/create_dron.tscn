	frame_container.custom_minimum_size = Vector2(300, 120)
	frame_container.size_flags_horizontal = Control.SIZE_EXPAND_FILL
	
	var frame_hbox = HBoxContainer.new()
	frame_hbox.name = \"FrameHBox\"
